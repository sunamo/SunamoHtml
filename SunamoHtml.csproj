<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Version>24.1.24.1</Version>
    <Description>Many ways to work with XML (extensible markup language)</Description>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>$(DefineConstants);ASYNC</DefineConstants>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DefineConstants>$(DefineConstants);ASYNC</DefineConstants>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="SunamoCollections" Version="24.1.24.1" />
    <PackageReference Include="SunamoString" Version="24.1.22.1" />
    <PackageReference Include="SunamoUri" Version="24.1.14.1" />
    <PackageReference Include="SunamoXml" Version="24.1.14.1" />
  </ItemGroup>
</Project>