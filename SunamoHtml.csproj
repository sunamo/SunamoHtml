<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Version>24.1.10.1</Version>
    <Description>Many ways to work with XML (extensible markup language)</Description>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>$(DefineConstants);ASYNC</DefineConstants>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DefineConstants>$(DefineConstants);ASYNC</DefineConstants>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="HtmlAgilityPack" Version="1.11.57" />
    <PackageReference Include="SunamoCollections" Version="24.1.10.1" />
    <PackageReference Include="SunamoFileIO" Version="24.1.12.3" />
    <PackageReference Include="SunamoI18N" Version="24.1.10.1" />
    <PackageReference Include="SunamoStringGetLines" Version="24.1.11.1" />
    <PackageReference Include="SunamoStringReplace" Version="24.1.10.1" />
    <PackageReference Include="SunamoStringSplit" Version="24.1.11.1" />
    <PackageReference Include="SunamoValues" Version="24.1.10.1" />
    <PackageReference Include="SunamoDelegates" Version="24.1.10.1" />
    <PackageReference Include="SunamoRegex" Version="24.1.10.1" />
    <PackageReference Include="SunamoXml" Version="24.1.10.1" />
  </ItemGroup>
</Project>